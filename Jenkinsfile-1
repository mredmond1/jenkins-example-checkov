pipeline {
  agent {
    docker { image 'bridgecrew/checkov:latest' }
  }
  stages {
    stage('verify version') {
      steps {
          sh 'checkov --version'
         }
       }
    stage('analyze compliant') {
      steps {
          sh 'checkov -d compliant --quiet'
        }
      }
     }
   }
 
  
